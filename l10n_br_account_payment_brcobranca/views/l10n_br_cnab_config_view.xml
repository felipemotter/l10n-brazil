<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="l10n_br_cnab_config_form_view" model="ir.ui.view">
        <field
            name="name"
        >l10n_br_account_payment_brcobranca.cnab.config.form.view</field>
        <field name="model">l10n_br_cnab.config</field>
        <field name="priority">99</field>
        <field
            name="inherit_id"
            ref="l10n_br_account_payment_order.l10n_br_cnab_config_form_view"
        />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='cnab_config']" position="after">
                <page name="config_brcobranca" string="Configuração BRCobrança">
                    <group name="main">
                        <field name="brcobranca_modelo" />
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
